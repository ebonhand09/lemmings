; These sections are used to fill the GIME to position the data that follows
; Format:	(something)_page	load (GIMEREG)
;		(something)		load (VIRT MEM LOC)

section	.program_code_physical_map		load	FFA6
section	program_code				load	C000	; $30

section module_gfx
section .bss

section	.program_code_stack			load	FD00

entry	ProgramCode
